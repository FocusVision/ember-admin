{{yield (hash
  column=(component
    'admin-table/admin-table-column'
    table=this
  )
)}}

<thead>
  <tr>
    {{#each columns as |column|}}
      <th>{{column.label}}</th>
    {{/each}}
  </tr>
</thead>

<tbody>
  {{#each normalizedModel as |row|}}
    <tr onclick={{action 'onRowClick' row}}>
      {{#each columns as |column|}}
        {{admin-table/admin-table-cell
          model=row
          column=column
        }}
      {{/each}}
    </tr>
  {{/each}}
</tbody>
