{{yield (hash
  column=(component
    'admin-table/admin-table-column'
    registrar=this
  )
  actionsColumn=(component
    'admin-table/admin-table-column'
    registrar=this
  )
)}}

<thead>
  <tr>
    {{#each columns as |column|}}
      <th>{{column.label}}</th>
    {{/each}}
  </tr>
</thead>

<tbody>
  {{#each normalizedModel as |row|}}
    <tr>
      {{#each columns as |column|}}
        {{admin-table/admin-table-cell column=column model=row options=options}}
      {{/each}}
    </tr>
  {{/each}}
</tbody>
