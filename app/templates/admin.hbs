<div class="ember-admin">
  <h2>Admin</h2>
  {{#each filteredModels as |name|}}
    <div data-test='models-list'>
      {{#link-to
        'model-records'
        name
        data-test=(concat 'models-list-item-' name)}}
        {{name}}
      {{/link-to}}
    </div>
  {{/each}}
  {{outlet}}
</div>
